@{
    ViewBag.Title = "משלוח חדש";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@Html.Partial("_navLists")

<div class="row">
    <div class="col-md-12">
        <h4>בחר יצירת משלוח עבור ארגון</h4>

        <ul class="list-group">
            @foreach (var org in ViewBag.Orgs)
            {
                <li class="list-group-item">
                    <a href="@Url.Action("Create", "S", new { orgid = org.OrgId.ToString() })"><i class="fa fa-hand-o-left fa-fw"></i>@org.Name</a></li> 

            }
        </ul>
    </div>
</div>
