@model Michal.Project.Models.OrderView

<div class="row">
    <div class=" col-md-12 ">
        <div class="row">
            <div class=" col-md-9 col-sm-12  col-xs-12  ">
                @Html.Partial("_rantoolbox")
            </div>
            <div class=" col-md-3  col-sm-12 col-xs-12">
                @Html.Partial("_qrcode")
            </div>
        </div>
    </div>
</div>
@Html.Partial("_StatusConfirm", Model.Status)
<div class="row">
    <div class="col-lg-9">
        @Html.Partial("_orderDetail")
    </div>
    <div class="col-lg-3">
        <div class="visible-lg" style="position: relative; top: 40px">

            @Html.Cyrcle(@Model.ShippingVm.StatusPresent, "big")
        </div>
    </div>
</div>

@Html.Partial("_locations", Model.ShippingVm)

<div class="row">
    <div class="col-md-12">
        @Html.Partial("_comments")
    </div>
</div>
<div class="row visible-md visible-xs  visible-sm">
    <div class="col-md-12">
        <div class="pull-right">
            @Html.Cyrcle(@Model.ShippingVm.StatusPresent, "")
        </div>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        @Html.Partial("_TimeLinePartial", Model.TimeLineVms)
    </div>
</div>


