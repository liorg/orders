@model Michal.Project.Models.OrderView


<div class="row">
    <div class=" col-md-12">
        <div class=" col-md-9">
            <div class="rantoolbox1 col-md-12">
                <nav>
                    <ul>
                        <!-- http://www.flaticon.com/free-icon/facebook-placeholder-for-locate-places-on-maps_34250-->
                        <li><a href="#"><img  alt="שלח ב whatsup" src="data:image/png;base64,"/></a></li>
                        <li><a href="#"><img  alt="היעד" src="data:image/png;base64,"/></a></li>
                         <li><a href="@Url.Action("Print", "S", new { id = @Model.ShippingVm.Id })"><img alt="הדפס" src="data:image/png;base64," /></a></li>
